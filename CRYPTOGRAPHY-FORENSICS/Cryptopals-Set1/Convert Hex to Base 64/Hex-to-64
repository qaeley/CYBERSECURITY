import base64

target_data = "SSdtIGtpbGxpbmcgeW91ciBicmFpbiBsaWtlIGEgcG9pc29ub3VzIG11c2hyb29t"
hex_data = bytes.fromhex("49276d206b696c6c696e6720796f757220627261696e206c696b65206120706f69736f6e6f7573206d757368726f6f6d")
base64_data = base64.b64encode(hex_data)

if base64_data.decode() == target_data:
    print("Challenge Completed")
else:
    print("Please try again")